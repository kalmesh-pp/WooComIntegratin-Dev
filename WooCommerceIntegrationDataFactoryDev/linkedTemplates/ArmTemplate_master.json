{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "WooCommerceIntegrationDataFactoryDev"
		},
		"KalmeshPC_SQLserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'KalmeshPC_SQLserver'"
		},
		"PP_PPMONWOOCOMSQL1_SQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PP_PPMONWOOCOMSQL1_SQLServer'"
		},
		"SFTP_WooComStaging_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SFTP_WooComStaging'"
		},
		"SFTP_WooComStaging_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "wabashstaging.sftp.wpengine.com"
		},
		"SFTP_WooComStaging_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "wabashstaging-syteline"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "WooCommerceIntegrationDataFactoryDev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SFTP_WooComStaging_password": {
						"value": "[parameters('SFTP_WooComStaging_password')]"
					},
					"KalmeshPC_SQLserver_connectionString": {
						"value": "[parameters('KalmeshPC_SQLserver_connectionString')]"
					},
					"PP_PPMONWOOCOMSQL1_SQLServer_connectionString": {
						"value": "[parameters('PP_PPMONWOOCOMSQL1_SQLServer_connectionString')]"
					},
					"SFTP_WooComStaging_properties_typeProperties_host": {
						"value": "[parameters('SFTP_WooComStaging_properties_typeProperties_host')]"
					},
					"SFTP_WooComStaging_properties_typeProperties_userName": {
						"value": "[parameters('SFTP_WooComStaging_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "WooCommerceIntegrationDataFactoryDev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'WooCommerceIntegrationDataFactoryDev_ArmTemplate_0')]"
			]
		}
	]
}